{
  "name": "@ledgerhq/wallet-api-client",
  "version": "0.5.2",
  "repository": "git@github.com:LedgerHQ/wallet-api.git",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "lib-es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/lib-es"
  ],
  "scripts": {
    "format:check": "prettier --check \"src\" \"tests\"",
    "format:fix": "prettier --write \"src\" \"tests\"",
    "lint": "eslint --cache --ext .ts \"src\" \"tests\"",
    "lint:fix": "eslint --cache --fix --ext .ts \"src\" \"tests\"",
    "dev": "tsc -p prod.tsconfig.json --watch",
    "build": "rm -rf lib/* lib-es/* && tsc -p prod.tsconfig.json && tsc -p prod-esm.tsconfig.json",
    "test": "nyc mocha",
    "doc": "typedoc --gitRevision main --out docs/reference src/index.ts",
    "doc:html": "typedoc --gitRevision main --out docs/html --plugin typedoc-plugin-rename-defaults src/index.ts"
  },
  "dependencies": {
    "@ledgerhq/wallet-api-core": "workspace:*",
    "bignumber.js": "^9.0.2",
    "json-rpc-2.0": "^1.1.0"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "^1.0.0",
    "@types/chai": "^4.3.3",
    "@types/chai-spies": "^1.0.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.8",
    "chai": "^4.3.6",
    "chai-spies": "^1.0.0",
    "eslint": "^8.26.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.19",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.8.4"
  }
}
