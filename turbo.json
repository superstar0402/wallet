{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "lib-es/**"],
      "inputs": ["src/**/*.ts"]
    },
    "lint": {
      "dependsOn": [],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": [],
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "format:fix": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["build", "lint"],
      "outputs": [],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "manifest-validator-cli#build": {
      "dependsOn": ["^build"],
      "outputs": ["bin/**"],
      "inputs": ["src/**/*.ts"]
    },
    "wallet-api-docs#dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "wallet-api-docs#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["pages/**", "*.js", "*.ts", "*.tsx", "*.css"]
    },
    "wallet-api-tools#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["src/**", "app/**", "public/**", "styles/**", "*.js"]
    },
    "wallet-api-tools#dev": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
